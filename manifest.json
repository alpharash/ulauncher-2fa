{
  "required_api_version": "^2.0.0",
  "name": "2FA Extension",
  "description": "2FA Authentication",
  "developer_name": "Martin Diphoorn",
  "icon": "images/icon.png",
  "options": {
    "query_debounce": 0.1
  },
  "preferences": [
    {
      "id": "tfa_kw",
      "type": "keyword",
      "name": "generate pin",
      "description": "keyword to activate this plugin",
      "default_value": "2fa"
    },
    {
      "id": "tfa_providers",
      "type": "text",
      "name": "providers",
      "description": "Every line is a key value pair: <br/>goolge=123123fAkE<br/>google2=321321FaKe<br/><br/><b>Make a backup of the above values!"
    }
  ]
}
